<div class="row">
  <div class="col-md-offset-3 col-md-6">
    <h1>Zoznam úloh pre žiaka <%= "#{current_user.first}" + ' ' + "#{current_user.last}" %></h1>
    <ul class="task-list">
      <% @tasks.each do |task| %>
        <li><%= link_to task.exercise.template.text, edit_task_path(task.id) %></li>
      <% end %>
    </ul>
  </div>
</div>